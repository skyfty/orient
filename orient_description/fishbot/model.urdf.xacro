<robot name="fishbot" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:arg name="namespace" default="" />
    <link name="base_footprint">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 -0 0" />
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
        </inertial>
    </link>
    <link name="base_link">
        <inertial>
            <mass value="7" />
            <origin xyz="-0.15 0 0.13" rpy="0 -0 0" />
            <inertia ixx="0.0594508" ixy="0" ixz="-2.77556e-17" iyy="0.150753" iyz="0" izz="0.172704" />
        </inertial>
        <visual>
            <origin xyz="-0.11 0 0.071" rpy="0 -0 0" />
            <geometry>
                <mesh filename="package://orient_description/fishbot/meshes/body.dae" scale="1 1 1" />
            </geometry>
        </visual>
        <visual>
            <origin xyz="-0.279 0.17 0.09" rpy="0 0 -3.14159" />
            <geometry>
                <mesh filename="package://orient_description/fishbot/meshes/caster_wheel.dae" scale="1 1 1" />
            </geometry>
        </visual>
        <visual>
            <origin xyz="-0.279 -0.17 0.09" rpy="0 0 -3.14159" />
            <geometry>
                <mesh filename="package://orient_description/fishbot/meshes/caster_wheel.dae" scale="1 1 1" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="-0.11 0 0.151" rpy="0 -0 0" />
            <geometry>
                <box size="0.398 0.268 0.16" />
            </geometry>
        </collision>
        <collision>
            <origin xyz="-0.136 0 0.248" rpy="0 -0 0" />
            <geometry>
                <box size="0.375 0.268 0.034" />
            </geometry>
        </collision>
        <collision>
            <origin xyz="-0.279 0.169 0.106" rpy="0 -0 0" />
            <geometry>
                <box size="0.1 0.07 0.07" />
            </geometry>
        </collision>
        <collision>
            <origin xyz="-0.279 -0.169 0.106" rpy="0 -0 0" />
            <geometry>
                <box size="0.1 0.07 0.07" />
            </geometry>
        </collision>
        <collision>
            <origin xyz="-0.3 0.169 0.04" rpy="0 -0 0" />
            <geometry>
                <sphere radius="0.04" />
            </geometry>
        </collision>
        <collision>
            <origin xyz="0.05 0.085 0.245" rpy="0 -1.15192 3.1415" />
            <geometry>
                <box size="0.03 0.026 0.016" />
            </geometry>
        </collision>
        <collision>
            <origin xyz="-0.3 -0.169 0.04" rpy="0 -0 0" />
            <geometry>
                <sphere radius="0.04" />
            </geometry>
        </collision>
        <collision>
            <origin xyz="0.01 -0.14 0.25" rpy="0 0 -1.5708" />
            <geometry>
                <box size="0.02 0.044 0.021" />
            </geometry>
        </collision>
        <collision>
            <origin xyz="0.08 -0.07 0.25" rpy="0 -0 0.785" />
            <geometry>
                <box size="0.02 0.044 0.021" />
            </geometry>
        </collision>
        <collision>
            <origin xyz="0.08 -0.01 0.25" rpy="0 0 -0.785" />
            <geometry>
                <box size="0.02 0.044 0.021" />
            </geometry>
        </collision>
        <collision>
            <origin xyz="0.08 0.04 0.25" rpy="0 -0 0" />
            <geometry>
                <box size="0.02 0.044 0.021" />
            </geometry>
        </collision>
        <collision>
            <origin xyz="0.01 0.14 0.25" rpy="0 -0 1.5708" />
            <geometry>
                <box size="0.02 0.044 0.021" />
            </geometry>
        </collision>
    </link>
    <link name="imu_link">
        <inertial>
            <mass value="0.1" />
            <origin xyz="0 0 0" rpy="0 -0 0" />
            <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001" />
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 -0 0" />
            <geometry>
                <box size="0.05 0.05 0.02" />
            </geometry>
            <material name="blue">
                <color rgba="0 0 0.8 1" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 -0 0" />
            <geometry>
                <box size="0.05 0.05 0.02" />
            </geometry>
        </collision>
    </link>
    <link name="left_wheel">
        <inertial>
            <mass value="3.34" />
            <origin xyz="2.77556e-17 -1.38778e-17 0" rpy="0 1.5708 -1.67887e-11" />
            <inertia ixx="0.00904583" ixy="0" ixz="0" iyy="0.00904583" iyz="0" izz="0.0167" />
        </inertial>
        <visual>
            <origin xyz="-0.071 -2.77556e-17 0" rpy="0 -0 0" />
            <geometry>
                <mesh filename="package://orient_description/fishbot/meshes/front_wheel.dae" scale="1 1 1" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="5.55112e-17 -2.77556e-17 0" rpy="0 1.5708 -1.67887e-11" />
            <geometry>
                <sphere radius="0.1" />
            </geometry>
        </collision>
    </link>
    <link name="lidar_link">
        <inertial>
            <mass value="0.01" />
            <origin xyz="0 0 0" rpy="0 -0 0" />
            <inertia ixx="1e-06" ixy="0" ixz="0" iyy="1e-06" iyz="0" izz="1e-06" />
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 -0 0" />
            <geometry>
                <mesh filename="package://orient_description/fishbot/meshes/sensors/R2000.dae" scale="0.001 0.001 0.001" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 -0 0" />
            <geometry>
                <cylinder radius="0.03" length="0.05" />
            </geometry>
        </collision>
    </link>
    <link name="right_wheel">
        <inertial>
            <mass value="3.34" />
            <origin xyz="-2.77556e-17 1.38778e-17 0" rpy="0 1.5708 -1.67887e-11" />
            <inertia ixx="0.00904583" ixy="0" ixz="0" iyy="0.00904583" iyz="0" izz="0.0167" />
        </inertial>
        <visual>
            <origin xyz="0.071 2.77556e-17 0" rpy="-3.14159 -0 -3.14159" />
            <geometry>
                <mesh filename="package://orient_description/fishbot/meshes/front_wheel.dae" scale="1 1 1" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="-5.55112e-17 2.77556e-17 0" rpy="0 1.5708 -1.67887e-11" />
            <geometry>
                <sphere radius="0.1" />
            </geometry>
        </collision>
    </link>
    <joint name="base_link_joint" type="fixed">
        <origin xyz="0 0 0" rpy="0 -0 0" />
        <axis xyz="0 0 0" />
        <parent link="base_footprint" />
        <child link="base_link" />
    </joint>
    <joint name="imu_fixed_joint" type="fixed">
        <origin xyz="0 0 0.15" rpy="0 -0 0" />
        <axis xyz="0 0 0" />
        <parent link="base_link" />
        <child link="imu_link" />
    </joint>
    <joint name="joint_tire_left" type="revolute">
        <origin xyz="0 0.163 0.1" rpy="0 -0 1.5708" />
        <axis xyz="1 -3.67321e-06 0" />
        <parent link="base_link" />
        <child link="left_wheel" />
        <dynamics damping="0" friction="0" />
        <limit effort="5" velocity="10" lower="-100" upper="100" />
    </joint>
    <joint name="joint_tire_right" type="revolute">
        <origin xyz="0 -0.163 0.1" rpy="0 -0 1.5708" />
        <axis xyz="1 -3.67321e-06 0" />
        <parent link="base_link" />
        <child link="right_wheel" />
        <dynamics damping="0" friction="0" />
        <limit effort="5" velocity="10" lower="-100" upper="100" />
    </joint>
    <joint name="lidar_fixed_joint" type="fixed">
        <origin xyz="0 0 0.37" rpy="0 -0 0" />
        <axis xyz="0 0 0" />
        <parent link="base_link" />
        <child link="lidar_link" />
    </joint>

    <ros2_control name="GazeboSimSystem" type="system">
        <hardware>
            <plugin>orient_gazebo_plugins/GazeboSimSystem</plugin>
        </hardware>
        <joint name="joint_tire_left">
            <command_interface name="velocity">
                <param name="min">-1</param>
                <param name="max">1</param>
            </command_interface>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="joint_tire_right">
            <command_interface name="velocity">
                <param name="min">-1</param>
                <param name="max">1</param>
            </command_interface>
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
    </ros2_control>

    <gazebo>
        <plugin filename="orient_gazebo_plugins-system" name="orient_gazebo_plugins::GazeboSimROS2ControlPlugin">
            <ros>
                <namespace>$(arg namespace)</namespace>
            </ros>
            <parameters>$(find orient_description)/fishbot/params/controller.yaml</parameters>
        </plugin>
    </gazebo>
</robot>
